{
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "rangeStart": 0,
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": [
                "*.wxss",
                "*.acss"
            ],
            "options": {
                "parser": "css"
            }
        },
        {
            "files": [
                "*.wxml",
                "*.axml"
            ],
            "options": {
                "parser": "html"
            }
        },
        {
            "files": [
                "*.wxs",
                "*.sjs"
            ],
            "options": {
                "parser": "babel"
            }
        }
    ]
}